{
	"manifest_version": 2,
	"browser": true,
	"chrome": true,
	"name": "Better Vocab",
	"browser_action": {
		"default_popup": "index.html",
		"default_title": "Open the popup"
	},
	"permissions": [ "activeTabs", "index.html" ],
	"author": "Darwin",
	"version": "1.0.1",
	"description": "Replace new tab screen with English word",
	"icons": {
		"16": "darwinFav16.ico",
		"48": "darwinFav48.ico",
		"128": "darwinFav128.ico"
	},
	"web_accessible_resources": [ "index.html" ]
}
